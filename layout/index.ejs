
  <!-- hero area -->
  <section class="hero-area bg-primary overflow-hidden" id="parallax">
    <div class="container">
      <div class="row">
        <div class="col-lg-11 mx-auto">
          <h1 class="text-white font-tertiary"><%= config.homepage.banner.title || theme.homepage.banner.title%></h1>
        </div>
      </div>
    </div>
    <div class="layer-bg w-100">
      <img class="img-fluid w-100" src="/css/images/illustrations/leaf-bg.png" alt="bg-shape">
    </div>
    <div class="layer" id="l2">
      <img src="/css/images/illustrations/dots-cyan.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l3">
      <img src="/css/images/illustrations/leaf-orange.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l4">
      <img src="/css/images/illustrations/dots-orange.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l5">
      <img src="/css/images/illustrations/leaf-yellow.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l6">
      <img src="/css/images/illustrations/leaf-cyan.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l7">
      <img src="/css/images/illustrations/dots-group-orange.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l8">
      <img src="/css/images/illustrations/leaf-pink-round.svg" alt="bg-shape">
    </div>
    <div class="layer" id="l9">
      <img src="/css/images/illustrations/leaf-cyan-2.svg" alt="bg-shape">
    </div>
  </section>
  <!-- /hero area -->

<% if(config.homepage.about.enable || theme.homepage.about.enable) { %>
<!-- about -->
<section class="section">
  <div class="container">
    <div class="row">
      <div class="col-lg-10 mx-auto text-center">
        <p class="font-secondary lead text-dark"><%= config.homepage.about.content || theme.homepage.about.content %></p>
        <% if(config.homepage.about.button.enable || theme.homepage.about.button.enable) { %>
          <a href="<%= config.homepage.about.button.url%>" class="btn btn-transparent"><%= config.homepage.about.button.label%></a>
        <% } %>
      </div>
    </div>
  </div>
</section>
<!-- /about -->
<% } %>

<% if(config.homepage.skill.enable || theme.homepage.skill.enable) { %>
<!-- skills -->
<section class="section">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-title"><%= config.homepage.skill.title || theme.homepage.skill.title%></h2>
      </div>
      <% config.homepage.skill.item.forEach((item) => { %>
        <div class="col-lg-3 col-sm-6 mb-4">
          <div class="card shadow text-center">
            <div class="position-relative rounded-top progress-wrapper" data-color="<%= item.color%>">
              <div class="wave" data-progress="<%= item.progress%>"></div>
            </div>
            <div class="card-footer bg-white">
              <h4 class="card-title"><%= item.title%>(<%= item.progress%>)</h4>
            </div>
          </div>
        </div>
      <%  }) %>
    </div>
  </div>
</section>
<!-- /skills -->
<% } %>

<% if(config.homepage.blog.enable) { %>
<!-- blog -->
<section class="section">
  <div class="container">
    <div class="row">
      <div class="col-12 text-center">
        <h2 class="section-title"><%= config.homepage.blog.title%></h2>
      </div>
      <% site.posts.forEach((post,i) => {%>
        <% if(i<=2) { %>
          <%- partial('_widget/post', {post: post}) %>
        <% }%>
      <% }) %>
      <div class="col-lg-10 mx-auto text-center">
        <a href="<%= config.homepage.blog.button.url%>" class="btn btn-transparent"><%= config.homepage.blog.button.label%></a>
      </div>
    </div>
  </div>
</section>
<!-- /blog -->
<% } %>

<% if(config.homepage.weekly.enable) { %>
  <!-- weekly -->
  <section class="section">
    <div class="container">
      <div class="row">
        <div class="col-12 text-center">
          <h2 class="section-title"><%= config.homepage.weekly.title%></h2>
        </div>
        <% site.posts.forEach((post,i) => {%>
          <% if(i<=2) { %>
            <%- partial('_widget/post', {post: post}) %>
          <% }%>
        <% }) %>
        <div class="col-lg-10 mx-auto text-center">
          <a href="<%= config.homepage.weekly.button.url%>" class="btn btn-transparent"><%= config.homepage.weekly.button.label%></a>
        </div>
      </div>
    </div>
  </section>
  <!-- /weekly -->
  <% } %>



  <% if(config.homepage.team.enable || theme.homepage.team.enable) { %>
  <!-- testimonial -->
  <section class="section bg-primary position-relative testimonial-bg-shapes">
    <div class="container">
      <div class="row">
        <div class="col-12 text-center">
          <h2 class="section-title text-white mb-5"><%= config.homepage.team.title%></h2>
        </div>
        <div class="col-lg-10 mx-auto testimonial-slider">
          <% config.homepage.team.item.forEach((item,i) => {%>
          <!-- slider-item -->
          <div class="text-center testimonial-content">
            <i class="ti-quote-right text-white icon mb-4 d-inline-block"></i>
            <p class="text-white mb-4"><%= item.content%></p>
            <img width="150" class="img-fluid rounded-circle mb-4 d-inline-block" src="<%= item.image%>" alt="client-image">
            <h4 class="text-white"><%= item.name%></h4>
            <h6 class="text-light mb-4"><%= item.designation%></h6>
          </div>
          <% }) %>
        </div>
      </div>
    </div>
    <!-- bg shapes -->
    <img src="/css/images/backgrounds/map.png" alt="map" class="img-fluid bg-map">
    <img src="/css/images/illustrations/dots-group-v.svg" alt="bg-shape" class="img-fluid bg-shape-1">
    <img src="/css/images/illustrations/leaf-orange.svg" alt="bg-shape" class="img-fluid bg-shape-2">
    <img src="/css/images/illustrations/dots-group-sm.svg" alt="bg-shape" class="img-fluid bg-shape-3">
    <img src="/css/images/illustrations/leaf-pink-round.svg" alt="bg-shape" class="img-fluid bg-shape-4">
    <img src="/css/images/illustrations/leaf-cyan.svg" alt="bg-shape" class="img-fluid bg-shape-5">
  </section>
  <!-- /testimonial -->
  <% } %>