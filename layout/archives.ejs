<%- partial('_partial/page-title') %>

<main class="app-body" id="archives">
  <% let years = {}; %>
  <%
    site.posts.each(post => {
      const y = post.date.year();
      if (!years[y]) {
        years[y] = [];
      }
      years[y].push(post);
    })
  %>
  <% for (let year of Object.keys(years).reverse()) { %>
    <%- partial('_partial/archives', { year: year, posts: years[year].reverse() }) %>
  <% } %>
</main>